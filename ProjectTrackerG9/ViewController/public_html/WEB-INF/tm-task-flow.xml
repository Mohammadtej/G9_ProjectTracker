<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="tm-task-flow">
    <default-activity>tmDashboard</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>TmTaskFlowBean</managed-bean-name>
      <managed-bean-class>view.TmTaskFlowBean</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <view id="tmDashboard">
      <page>/tmPages/tmDashboard.jsf</page>
    </view>
  </task-flow-definition>
</adfc-config>
